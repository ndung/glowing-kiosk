<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ics.ssk.ego.model.KAIStation" table="kai_station">
        <id name="id" type="string">
            <column name="ID" length="10" />
            <generator class="assigned" />
        </id>
        <many-to-one name="kaiStationGroup" class="com.ics.ssk.ego.model.KAIStationGroup" fetch="select">
            <column name="ID_GROUP" length="10" not-null="true" />
        </many-to-one>
        <property name="description" type="string">
            <column name="DESCRIPTION" length="50" not-null="true" />
        </property>
        <property name="original" type="string">
            <column name="ORIGINAL" length="1" not-null="true" />
        </property>
        <property name="destination" type="string">
            <column name="DESTINATION" length="1" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
